LET du = DOCUMENT(DeactivatedUser, @username) && FAIL('User does exists')

INSERT {
    username: @username,
    email: @email,
    password: @password,
    name: @name,
    bio: @bio,
    birthday: @birthday,
    profile_img: @profile_img,
    created_at: DATE_NOW(),
    updated_at: DATE_NOW(),
    is_active: @is_active || false
} Into User
